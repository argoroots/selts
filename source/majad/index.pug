extends /layout


block script
    script(src='https://maps.googleapis.com/maps/api/js?key=AIzaSyCLvF-XJHdyb1STAF38EukXHJpoNC_NwqU', type='application/javascript')


block content
    each u in self.file.majad
        h1.row
            a(href='/' + u.path + '/')
                .col-xs-12.col-sm-9= u.title
        .list-item.hidden(data-geo=u.geo, data-info=u.title)

extends /layout


block script
    script(src='https://maps.googleapis.com/maps/api/js?key=AIzaSyDKUxMa3cmiecm331dv0_gTDE0wKkuxpsw', type='application/javascript')


block content
    each u in self.file.majad
        h1.row
            a(href='/' + u.path + '/')
                .col-xs-12.col-sm-9= u.title
        if u.geo
            .map-item.hidden(data-geo=u.geo, data-info=u.title)

    .row
        #map.col-xs-12
